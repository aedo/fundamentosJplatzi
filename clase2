function canWatchStarWars7(name, age){
  if (age >= pgStarWars7){
    alert(`${name} puede pasar a ver ${starWars7}`)
  }
  else {
    `${name} no puede pasar a ver ${starWars7}. Tiene ${age}, y necesita tener ${pgStarWars7}`
  }
}

